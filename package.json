{
  "name": "burn-or-not",
  "displayName": "burn-or-not",
  "description": "Test extension",
  "publisher": "robertohuertasm",
  "repository": {
    "type": "git",
    "url": "https://marketplace.visualstudio.com/items?itemName=robertohuertasm.burn-or-not"
  },
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "burn-or-not.helloWorld",
        "title": "Burn: Hello"
      },
      {
        "command": "burn-or-not.staticAnalysis",
        "title": "Burn: Start Static Analysis Server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "biome check --apply .",
    "format": "biome format --write .",
    "testold": "node ./out/test/runTest.js",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.19.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  }
}
